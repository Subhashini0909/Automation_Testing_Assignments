package java_;

public class Multiplication_Table {

	public static void main(String[] args) 
	
	{
		int n1=10;
		System.out.println("FOR LOOP");
		for(int i=1; i<=n1; i++) {
			for(int j =1; j<=n1; j++)
			{
				System.out.print("   " + i*j);
			}
			System.out.println();
		}
		
		System.out.println();
		System.out.println("While LOOP");
		System.out.println();
		int n=1;
		while(n<=n1)
		{
			int m=1;
			while( m<=n1)
			{
				System.out.print("   " + n*m);
				m++;
			}
			n++;
			System.out.println();
		}
		
		System.out.println();
		System.out.println("Do While LOOP");
		System.out.println();
		int x=1;
		do
		{
			System.out.println();
		}
		while(x<=5);

	}
	
}
