package Java_Assignments;

public class Pattern_RightAngle_Alphabet_15d {

	public static void main(String[] args) 
	{
		for(int i = 1; i<=5; i++)
		{
			for(int j = 1; j<=i; j++)
			{
				char letter = (char) ('A' + j - 1);
				System.out.print(letter + " ");
			}
			System.out.println();
		}

	}

}
